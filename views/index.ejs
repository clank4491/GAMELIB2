<% include header %>
<% if(games.length){ %>
    <% games.forEach(function(games){ %>
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="text-muted pull-right">
            <%= games.published %>            
            </div>
            <%= games.title %>
        </div>
        <div class="panel-body">
            <%= games.body %>
        </div>
    </div>
    <% }) %>
    <% } else { %>
        No entries! <a href="/new-games"> Add a game?</a>
    <% } %>

<% include footer %>